{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "intelligent-analysis"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/css/bootstrap.min.css",
        "assets/font-awesome/css/font-awesome.css",
        "assets/css/animate.css",
        "assets/css/balloon.min.css",
        "assets/css/plugins/sweetalert/sweetalert.css",
        "assets/css/plugins/cropper/cropper.css",
        "assets/css/plugins/jsTree/style.css",
        "assets/css/plugins/steps/jquery.steps.css",
        "assets/css/plugins/datatables/datatables.css",
        "assets/css/plugins/bootstrapdatetimepicker/build_standalone.css",
        "assets/css/plugins/iCheck/custom.css",
        "assets/css/plugins/chosen/bootstrap-chosen.css",
        "assets/css/plugins/awesome-bootstrap-checkbox/awesome-bootstrap-checkbox.css",
        "assets/css/style.css"
      ],
      "scripts": [
        "assets/js/jquery-3.1.1.min.js",
        "assets/js/plugins/bootstrapdatetimepicker/bootstrap-datetimepicker.min.js",
        "assets/js/plugins/bootstrap-timepicker/bootstrap-timepicker.js",
        "assets/js/plugins/aliyun/Aliyun-OSS.js",
        "assets/js/plugins/sweetalert/sweetalert.min.js",
        "assets/js/plugins/cropper/cropper.js",
        "assets/js/plugins/jsTree/jstree.min.js",
        "assets/js/plugins/switchery/switchery.js",
        "assets/js/plugins/datatables/datatables.js",
        "assets/js/plugins/echarts/echarts.js",
        "assets/js/plugins/echarts/echarts-wordcloud.min.js",
        "assets/js/plugins/md5/md5.js",
        "assets/js/plugins/nouslider/jquery.nouislider.min.js",
        "assets/js/plugins/rangeSlider/ion.rangeSlider.min.js",
        "assets/js/plugins/jasny/jasny-bootstrap.min.js",
        "assets/js/plugins/steps/jquery.steps.min.js",
        "assets/js/plugins/chosen/chosen.jquery.js",
        "assets/js/plugins/validate/jquery.validate.min.js",
        "assets/js/iconfont/iconfont.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
